users/new <br>

<h1>Welcome. Please Sign Up Here!</h1>

<%= form_tag do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>
  <%= label_tag :email %>
  <%= text_field_tag :email %><br>
  <%= label_tag :password %>
  <%= text_field_tag :password %><br>
  <%= label_tag :role %>
  <%= text_field_tag :role %><br>
  <%= submit_tag 'Log In' %><br>
<% end %>
