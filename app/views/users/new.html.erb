<h1>Welcome. Please Sign Up Here!</h1>


<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password %>
  <%= f.text_field :password %><br>
  <%= f.submit 'Sign Up' %><br>
<% end  %>
